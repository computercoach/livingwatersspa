        {
          "_params": {
            "showTitle": true,
            "showText": true,
            "showButtons": false,
            "showArrow": false,
            "contentAlign": "center",
            "bgImageRadio": true,
            "bgImage": "@PROJECT_PATH@/assets/images/therapy-pool-648x567.jpg",
            "parallax": false,
            "bgColorRadio": false,
            "bgColor": "#a07e49",
